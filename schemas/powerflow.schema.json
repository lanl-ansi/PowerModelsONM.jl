  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "schemas/powerflow",
  "title": "Powerflow",
  "type": "array",
  "x-tags": [
    "output"
  ],
  "description": "The schema for the powerflow outputs from [`optimize_dispatch`](@ref optimize_dispatch), which include real and reactive power setpoints at generation objects, which include diesel generators (`generator`), substations/grid/feeder (`voltage_source`), solar PV (`solar`), or energy storage (`storage`), and voltage magnitudes at buses. All values are in SI units.",
  "examples": [
    [
      {
        "voltage_source": {
          "source": {
            "real power setpoint (kW)": [
              581.8173560045832,
              468.8619242602074,
              708.8039492196247
            ],
            "reactive power setpoint (kVar)": [
              244.09972136399347,
              32.25338488759224,
              40.35427973972425
            ]
          }
        },
        "solar": {
          "pv1": {
            "real power setpoint (kW)": [
              64.49371884320102,
              64.507863885424,
              64.49849984139108
            ],
            "reactive power setpoint (kVar)": [
              41.23181415943054,
              41.23340639500285,
              41.23312440672468
            ]
          }
        },
        "bus": {
          "611": {
            "voltage (V)": [
              2.5217499128997907
            ]
          }
        }
      }
    ]
  ],
  "items": {
    "type": "object",
    "additionalProperties": false,
    "properties": {
      "generator": {
        "type": "object",
        "additionalProperties": false,
        "description": "Generation types: diesel generators (`generator`), or substations (`voltage_source`), or solar PV (`solar`), or energy storage (`storage`)",
        "patternProperties": {
          "^.+$": {
            "type": "object",
            "additionalProperties": false,
            "description": "generation object",
            "properties": {
              "real power setpoint (kW)": {
                "type": "array",
                "description": "real power setpoints in kW",
                "items": {
                  "type": "number"
                }
              },
              "reactive power setpoint (kVar)": {
                "type": "array",
                "description": "reactive power setpoint in kVar",
                "items": {
                  "type": "number"
                }
              }
            }
          }
        }
      },
      "voltage_source": {
        "type": "object",
        "additionalProperties": false,
        "description": "Generation types: diesel generators (`generator`), or substations (`voltage_source`), or solar PV (`solar`), or energy storage (`storage`)",
        "patternProperties": {
          "^.+$": {
            "type": "object",
            "additionalProperties": false,
            "description": "generation object",
            "properties": {
              "real power setpoint (kW)": {
                "type": "array",
                "description": "real power setpoints in kW",
                "items": {
                  "type": "number"
                }
              },
              "reactive power setpoint (kVar)": {
                "type": "array",
                "description": "reactive power setpoint in kVar",
                "items": {
                  "type": "number"
                }
              }
            }
          }
        }
      },
      "solar": {
        "type": "object",
        "additionalProperties": false,
        "description": "Generation types: diesel generators (`generator`), or substations (`voltage_source`), or solar PV (`solar`), or energy storage (`storage`)",
        "patternProperties": {
          "^.+$": {
            "type": "object",
            "additionalProperties": false,
            "description": "generation object",
            "properties": {
              "real power setpoint (kW)": {
                "type": "array",
                "description": "real power setpoints in kW",
                "items": {
                  "type": "number"
                }
              },
              "reactive power setpoint (kVar)": {
                "type": "array",
                "description": "reactive power setpoint in kVar",
                "items": {
                  "type": "number"
                }
              }
            }
          }
        }
      },
      "storage": {
        "type": "object",
        "additionalProperties": false,
        "description": "Generation types: diesel generators (`generator`), or substations (`voltage_source`), or solar PV (`solar`), or energy storage (`storage`)",
        "patternProperties": {
          "^.+$": {
            "type": "object",
            "additionalProperties": false,
            "description": "generation object",
            "properties": {
              "real power setpoint (kW)": {
                "type": "array",
                "description": "real power setpoints in kW",
                "items": {
                  "type": "number"
                }
              },
              "reactive power setpoint (kVar)": {
                "type": "array",
                "description": "reactive power setpoint in kVar",
                "items": {
                  "type": "number"
                }
              }
            }
          }
        }
      },
      "bus": {
        "type": "object",
        "additionalProperties": false,
        "description": "Bus objects indexed by name",
        "patternProperties": {
          "^.+$": {
            "type": "object",
            "description": "Bus object",
            "additionalProperties": false,
            "properties": {
              "voltage (V)": {
                "type": "array",
                "description": "Voltage magnitudes in Volts",
                "items": {
                  "type": "number"
                }
              }
            }
          }
        }
      }
    }
  }
}