{
  "title": "output",
  "type": "object",
  "x-tags": [
    "output"
  ],
  "description": "Main output schema for PowerModelsONM",
  "properties": {
    "Simulation time steps": {
      "type": "array",
      "items": {
        "type": "string",
        "format": "date-time"
      }
    },
    "Load served": {
      "type": "object",
      "properties": {
        "Feeder load (%)": {
          "type": "array",
          "items": {
            "type": "number",
            "format": "float"
          }
        },
        "Microgrid load (%)": {
          "type": "array",
          "items": {
            "type": "number",
            "format": "float"
          }
        },
        "Bonus load via microgrid (%)": {
          "type": "array",
          "items": {
            "type": "number",
            "format": "float"
          }
        }
      }
    },
    "Generator profiles": {
      "type": "object",
      "properties": {
        "Grid mix (kW)": {
          "type": "array",
          "items": {
            "type": "number"
          }
        },
        "Solar DG (kW)": {
          "type": "array",
          "items": {
            "type": "number"
          }
        },
        "Energy storage (kW)": {
          "type": "array",
          "items": {
            "type": "number"
          }
        },
        "Diesel DG (kW)": {
          "type": "array",
          "items": {
            "type": "number"
          }
        }
      }
    },
    "Voltages": {
      "type": "object",
      "properties": {
        "Min voltage (p.u.)": {
          "type": "array",
          "items": {
            "type": "number"
          }
        },
        "Mean voltage (p.u.)": {
          "type": "array",
          "items": {
            "type": "number"
          }
        },
        "Max voltage (p.u.)": {
          "type": "array",
          "items": {
            "type": "number"
          }
        }
      }
    },
    "Storage SOC (%)": {
      "type": "array",
      "items": {
        "type": "number"
      }
    },
    "Device action timeline": {
      "$ref": "./device_action_timeline.v1.json"
    },
    "Powerflow output": {
      "$ref": "./powerflow.v1.json"
    },
    "Summary statistics": {
      "$ref": "./summary_statistics.v1.json"
    },
    "Events": {
      "$ref": "./events.v1.json"
    },
    "Protection settings": {
      "$ref": "./protection_settings.v1.json"
    },
    "Runtime arguments": {
      "$ref": "./runtime_arguments.v1.json"
    },
    "Fault currents": {
      "$ref": "./fault_study_results.v1.json"
    },
    "Small signal stable": {
      "type": "array",
      "items": {
        "type": "boolean"
      }
    },
    "Runtime timestamp": {
      "type": "string",
      "description": "Time at which ONM algorithm was executed",
      "format": "date-time"
    },
    "Switch changes": {
      "type": "array",
      "description": "List of switches whose state changed between timesteps",
      "items": {
        "type": "array",
        "items": {
          "type": "string"
        }
      }
    }
  }
}